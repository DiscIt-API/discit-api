{
  "name": "discit",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "db:init": "npx mikro-orm migration:create --initial",
    "db:create": "npx mikro-orm migration:create",
    "db:up": "npx mikro-orm migration:up",
    "db:down": "npx mikro-orm migration:down",
    "postinstall": "npm run build",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdleveille/discit.git"
  },
  "keywords": [],
  "author": "Chris Leveille",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdleveille/discit/issues"
  },
  "homepage": "https://github.com/cdleveille/discit#readme",
  "dependencies": {
    "@mikro-orm/core": "^4.5.9",
    "@mikro-orm/postgresql": "^4.5.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^4.5.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts"
    ]
  }
}